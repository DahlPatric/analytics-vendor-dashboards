				"token": "namespace_token",
				"defaultValue": "*",
				"items": [
					{
						"value": "*",
						"label": "All"
					}
				]
			},
			"dataSources": {
				"primary": "ds_search_1"
			}
		},
		"input_3": {
			"type": "input.dropdown",
			"title": "Domain",
			"options": {
				"token": "domain_token",
				"defaultValue": "*",
				"items": [
					{
						"value": "*",
						"label": "All"
					}
				]
			},
			"dataSources": {
