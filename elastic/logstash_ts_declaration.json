{
	"class": "Telemetry",
	"My_System": {
		"class": "Telemetry_System",
		"systemPoller": {
			"interval": 60,
			"actions": [{
				"includeData": {},
                "locations": {
                    "pools": {
                        "/Common/portal.app/*": {
                            "availabilityState": true,
                            "activeMemberCnt": true,
                            "members": {
                                ".*": {
                                    "addr": true,
                                    "monitorStatus": true
                                }
                            }
                        }
                    },
                    "virtualServers": {
                        "/Common/portal.app/*": {
                            "availabilityState": true,
                            "clientside.bitsIn": true,
                            "clientside.bitsOut": true,
                            "clientside.pktsIn": true,
                            "clientside.pktsOut": true,
                            "clientside.curConns": true,
                            "name": true,
                            "pool": true
                        }
                    }
                    }
            }]
		}
	},
	"My_Listener": {
		"class": "Telemetry_Listener",
		"port": 6514,
			"actions": [{
				"includeData": {},
                "locations": {
                    "Entity": {
                        "SystemMonitor": true
                    },
                    "AvgConcurrentConnections": true,
                    "MemoryHealth": true,
                    "CpuHealth": true,
                    "AvgThroughput": true
                }
            }
            ]
	},
    "My_ELK_Consumer": {
      "class": "Telemetry_Consumer",
      "type": "Generic_HTTP",
      "host": "206.124.129.135",
      "protocol": "http",
      "port": 8080,
      "path": "/",
      "method": "POST",
      "headers": [
           {
             "name": "content-type",
             "value": "application/json"
           }
       ]
   }
}
